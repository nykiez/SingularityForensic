<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Title.Header.Background" Color="#5e6164"/>
    <SolidColorBrush x:Key="Title.Header.Foreground" Color="White"/>
    
    <SolidColorBrush x:Key="Panel.Content.Background" Color="#f5f6f8"/>
    <SolidColorBrush x:Key="Panel.Content.BorderBrush" Color="#e5e5e5"/>
    <SolidColorBrush x:Key="Panel.Border" Color="#d7d7d7" />
    
    <SolidColorBrush x:Key="Window.TitleBrush" Color="#2a2e31"/>
    <SolidColorBrush x:Key="Window.NonActiveTitleBrush" Color="#808080"/>
    <SolidColorBrush x:Key="Window.StatusBar.Back" Color="#007acc" />
    <SolidColorBrush x:Key="Window.Border" Color="Black"/>

    <SolidColorBrush x:Key="Window.Background" Color="#f5f6f8"/>

    <SolidColorBrush x:Key="MessageBoxWindowTitleBrushKey" Color="#2a2e31"/>
    <SolidColorBrush x:Key="MessageBoxWindowBorderBrushKey" Color="Black"/>
    <SolidColorBrush x:Key="MessageBoxButtonBorderBrushKey" Color="#3f92f0"/>


    <SolidColorBrush x:Key="SureButtonBack" Color="#3f92f0"/>
    <SolidColorBrush x:Key="SureButtonFore" Color="White"/>
    <SolidColorBrush x:Key="DenyButtonBack" Color="#cdcdcd" />
    <SolidColorBrush x:Key="DenyButtonFore" Color="Black"/>
</ResourceDictionary>