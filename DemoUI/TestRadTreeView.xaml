<UserControl x:Class="DemoUI.TestRadTreeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
             xmlns:local="clr-namespace:DemoUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <telerik:RadTreeView SelectedItem="{Binding SelectedUnit}" ItemsSource="{Binding TreeUnits}" >
            <telerik:RadTreeViewItem Header="HAHA" >
                <telerik:RadTreeViewItem Header="HEHE" >
                    
                </telerik:RadTreeViewItem>
            </telerik:RadTreeViewItem>
        </telerik:RadTreeView>
    </Grid>
</UserControl>
