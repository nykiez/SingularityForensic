<UserControl x:Class="Singularity.UI.Previewers.Views.OutsideInPreviewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Singularity.Previewers.Views"
             xmlns:wfi="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             mc:Ignorable="d" Loaded="UserControl_Loaded"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <wfi:WindowsFormsHost x:Name="host" Grid.Row="1" >
            <wf:Panel x:Name="panel" SizeChanged="panel_SizeChanged"></wf:Panel>
        </wfi:WindowsFormsHost>
    </Grid>
</UserControl>
